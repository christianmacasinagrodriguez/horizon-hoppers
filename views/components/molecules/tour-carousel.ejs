<div class="swiper mySwiper2 mb-4">
  <div class="swiper-wrapper">
    <% images.forEach((image) => {%>
      <div class="swiper-slide relative">
        <div class="absolute top-5 right-5 z-50 text-red-900 flex gap-4 items-center justify-center">
          <a class=" bg-black w-9 h-9 bg-opacity-20 grid place-content-center rounded-full text-white hover:cursor-pointer hover:bg-white hover:text-emerald-700 hover:bg-opacity-100 transition-all duration-500" href="#">
            <i class="fa fa-heart w-full h-full text-sm" aria-hidden="true"></i>
          </a>
          <a class=" bg-black w-9 h-9 bg-opacity-20 grid place-content-center rounded-full text-white hover:cursor-pointer hover:bg-white hover:text-emerald-700 hover:bg-opacity-100 transition-all duration-500" href="#">
            <i class="fab fa-youtube w-full h-full text-sm" aria-hidden="true"></i>
          </a>
        </div>
        <img src="<%= image %>" class="h-[250px] md:h-[500px] lg:h-[500px] w-full object-scale-down object-center" />
      </div>
    <% }) %>
  </div>
</div>
<div thumbsSlider="" class="swiper mySwiper">
  <div class="swiper-wrapper">
    <% images.forEach((image) => {%>
      <div class="swiper-slide">
        <img src="<%= image %>" class="h-[50px] md:h-[100px] lg:h-[100px] w-full object-cover object-center"/>
      </div>
    <% }) %>
  </div>
</div>